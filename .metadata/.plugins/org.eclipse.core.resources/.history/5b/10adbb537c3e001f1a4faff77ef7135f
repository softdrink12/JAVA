	String type;
	
	int eat() {
		System.out.println(this.name + "먹는다");
		return (int)(Math.random()*5)+1;
	}
	
	void order(int money) {
		System.out.println(this.name + "주문한다");
		if(money < this.price) {
			System.out.println("금액 부족으로 주문 취소합니다");
		}
	}
	
	void cook() {
		
	}
	
}
